gg=xlabel('Time (s)');

gg=ylabel('velocidade subida (m/s)');


subplot(4,2,5)
gg=plot(val.tout,val.q(:,:));
set(gg,'LineWidth',1.5)
gg=xlabel('Time (s)');

gg=ylabel('raz√£o de picada (rad/s)');


subplot(4,2,6)
