function []=plots_sae(val)
%plots
f=figure();
f.Position = [50 100 800 800];

subplot(4,2,1)
gg=plot(val.tout,180/pi*val.de_step(:,:));
set(gg,'LineWidth',1.5)
gg=xlabel('Time (s)');

gg=ylabel('step input elevator (deg)');


